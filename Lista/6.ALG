Algoritmo "exercicio_seis"


Var

   v, m, vm: real

Inicio
       escreva ("Informe a velocidade do veículo: ")
       leia(v)
       vm <- 434.4 / v
       m <- v / 3.6
       
       escreval("O percurso de São Paulo até o Rio de Janeiro corresponde á:", vm, " h")
       escreval("Velocidade em m/s:", m:2:2)

Fimalgoritmo