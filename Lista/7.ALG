Algoritmo "exercicio_sete"

Var
   tem : real

Inicio

      escreva("Informe a temperatura ambiente atual: ")
      leia(tem)
      
      se (tem >= 25)  entao
         escreva("O tempo está muito quente!")

         senao
         escreva("O tempo está agradável!")
     fimse


Fimalgoritmo