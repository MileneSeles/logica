algoritmo "Exercicio_quatro"

var
    meta, ven : inteiro

inicio
        meta <- 20
        
       escreval("Digite quantas vendas foram realizadas: ")
       leia(ven)
       
       se ven >= meta  entao
       escreval("Parabéns você ganhou!")
       senao
       escreval("Você não ganhou")
       
       fimse

fimalgoritmo